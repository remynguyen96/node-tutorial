{{#each blog}}
<div class="row">
    {{#each this}}
    <div class="col s12 m4">
      <div class="card">
        <div class="card-image">
          <img src="/images/{{this.images}}">
          <span class="card-title">{{this.title}}</span>
          <a href="" class="btn-floating halfway-fab waves-effect waves-light red"><i class="material-icons">add</i></a>
        </div>
        <div class="card-content">
          <p>Price : <strong>{{this.price}}</strong></p>
        </div>
        <div class="card-action">
          <a href="list-blogs/{{this.slug}}" class="green accent-4 btn waves-effect waves-light">Detail</a>
        </div>
      </div>
    </div>
    {{/each}}
</div>
{{/each}}
